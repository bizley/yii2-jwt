{
  "name": "bizley/jwt",
  "description": "JWT integration for Yii 2",
  "type": "yii2-extension",
  "keywords": ["yii2", "jwt", "jws", "token", "authentication"],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Pawel Bizley Brzozowski",
      "email": "pawel@positive.codes"
    },
    {
      "name": "Dmitriy Demin",
      "email": "sizemail@gmail.com"
    }
  ],
  "support": {
    "source": "https://github.com/bizley/yii2-jwt"
  },
  "require": {
    "php": ">=7.1.0",
    "lcobucci/jwt": ">3.2 <4.0",
    "yiisoft/yii2": ">=2.0.14 <2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "bizley\\jwt\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "bizley\\tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
